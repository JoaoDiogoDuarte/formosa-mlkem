#ifndef POLYVEC_INVNTT_HH
#define POLYVEC_INVNTT_HH

#include "params.jahh"
#include "poly_invntt.jahh"

inline
fn polyvec_invntt(stack u16[KYBER_VECN] r) -> stack u16[KYBER_VECN]
{
  r[0:KYBER_N] = poly_invntt(r[0:KYBER_N]);
  r[KYBER_N:KYBER_N] = poly_invntt(r[KYBER_N:KYBER_N]);
  r[2*KYBER_N:KYBER_N] = poly_invntt(r[2*KYBER_N:KYBER_N]);

  return r;
}

#endif
