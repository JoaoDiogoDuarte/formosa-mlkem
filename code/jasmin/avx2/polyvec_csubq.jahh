#ifndef POLYVEC_CSUBQ_HH
#define POLYVEC_CSUBQ_HH

#include "params.jahh"
#include "poly_csubq.jahh"

inline
fn polyvec_csubq(stack u16[KYBER_VECN] r) -> stack u16[KYBER_VECN]
{
  r[0:KYBER_N] = poly_csubq(r[0:KYBER_N]);
  r[KYBER_N:KYBER_N] = poly_csubq(r[KYBER_N:KYBER_N]);
  r[2*KYBER_N:KYBER_N] = poly_csubq(r[2*KYBER_N:KYBER_N]);

  return r;
}

#endif
