#ifndef POLY_SUB_HH
#define POLY_SUB_HH

#include "params.jahh"

fn poly_sub(reg ptr u16[KYBER_N] rp ap bp) -> reg ptr u16[KYBER_N]
{
  reg u16 a;
  reg u16 b;
  reg u16 r;
  inline int i;

  for i = 0 to KYBER_N {
    a = ap[i];
    b = bp[i];
    r = a - b;
    rp[i] = r;
  }
  return rp;
}

#endif
