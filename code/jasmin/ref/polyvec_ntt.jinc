require "params.jinc"
require "poly_ntt.jinc"
require "polyvec_topolys.jinc"
require "polyvec_frompolys.jinc"

inline
fn __polyvec_ntt(stack u16[KYBER_VECN] r) -> stack u16[KYBER_VECN]
{
  stack u16[KYBER_N] r0 r1 r2;

  r0, r1, r2 = __polyvec_topolys(r);

  r0 = _poly_ntt(r0);
  r1 = _poly_ntt(r1);
  r2 = _poly_ntt(r2);

  r = __polyvec_frompolys(r0, r1, r2);

  return r;
}
