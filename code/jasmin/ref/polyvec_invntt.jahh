require "params.jahh"
require "poly_invntt.jahh"
require "polyvec_topolys.jahh"
require "polyvec_frompolys.jahh"

inline
fn polyvec_invntt(stack u16[KYBER_VECN] r) -> stack u16[KYBER_VECN]
{

  stack u16[KYBER_N] r0 r1 r2;

  r0, r1, r2 = polyvec_topolys(r);

  r0 = poly_invntt(r0);
  r1 = poly_invntt(r1);
  r2 = poly_invntt(r2);

  r = polyvec_frompolys(r0, r1, r2);

  return r;
}
